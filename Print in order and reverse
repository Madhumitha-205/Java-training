import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    Scanner sc = new Scanner(System.in);
        int N =sc.nextInt();
        int i;
        int j;
        for(i=1;i<=N;i++){
            System.out.print(i);
            System.out.print(" ");
        }
            for(j=N;j>=1;j--){
                System.out.print(j);
            System.out.print(" ");
            }
        
    }
}
